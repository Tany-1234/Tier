#include <functional>
#include<stdio.h>
#include<string.h>
#include <string>

int Subsequence(char s3[], char s1[]) {
    int i = 0, j = 0;
    while (i < strlen(s3) && j < strlen(s1)) {
        if (s3[i] == s1[j]) {
            i++;
        }
        j++;
    }
    if (i == strlen(s3)) {
        return 1;
    }
    else {
        return 0;
    }

}
int main() {
        char s1[100], s3[100];

        printf("Enter string s1: ");
        scanf("%s", s1);

        printf("Enter string s3: ");
        scanf("%s", s3);

        if (Subsequence(s3, s1))
            printf("True");
        else
            printf("False");

        return 0;
}
