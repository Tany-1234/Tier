#include<stdio.h>
#include<string.h>
 int main() {
     char s[100];
     char x[100];
     printf("Enter the string");
     scanf("%s",s);
     printf("Enter the string");
     scanf("%s",x);

     char *poss=strstr(s,x);
     if(poss!=NULL) {
         int index=poss-s;
         printf("Substring %d",index);
     }
     else {
         printf("Not Substring");
     }
     return 0;
 }
