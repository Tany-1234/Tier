#include <stdio.h>
#include <string.h>

int main() {
    char arr[100];
    char s1[100];
    int n, i, j;

    printf("Enter a string: ");
    scanf("%s", arr);

    n = strlen(arr);

    printf("\nAll Possible Rotations:\n");

    for(i = 0; i < n; i++) {
        for(j = 0; j < n; j++) {
            s1[j] = arr[(i + j) % n];
        }

        s1[n] = '\0';

        printf("%s\n", s1);
    }

    return 0;
}
