#include <stdio.h>
#include<string.h>
int main() {
    char s1[100], s2[100];
    int i, j, k;
    int n;

    printf("Enter s1: ");
    scanf("%s", s1);

    printf("Enter s2: ");
    scanf("%s", s2);

    n = strlen(s1);

    // Length check
    if (n != strlen(s2)) {
        printf("Not Rotated String");
        return 0;
    }
    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            if (s1[(i + j) % n] != s2[j])
                break;
            }
        if (j == n) {
            return 0;
        }
    }

    printf("Not Rotated String");

    return 0;
}
