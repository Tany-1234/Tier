#include<stdio.h>
int main() {
    int arr[]={2,6,9,4,5};
    int n=5;
    int max=arr[0];
    int min=arr[0];
    for(int i=1;i<n;i++) {
        if(arr[i]>max) {
            max=arr[i];
        }
        if(arr[i]<min) {
            min=arr[i];
        }
    }
    int diff=max-min;
    printf("Max = %d\n", max);
    printf("Min = %d\n", min);
    printf("Difference = %d\n", diff);
    return 0;

}
