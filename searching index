#include<stdio.h>
int main() {
    int arr[5] = {1,2,3,4,5};
    int n=5;
    int target;
    printf("Enter the value");
    scanf("%d",&target);
    for(int i=0;i<n;i++) {
        if (arr[i]==target) {
            printf(" index of %d ",i);
            return 0;
        }
        else if (arr[i]>target) {
            printf(" index of %d ",i);
            return 0;
        }
    }
    printf("insert at index : %d",n);
    return 0;
}
