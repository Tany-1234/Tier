#include<stdio.h>
int main () {
    int n,m,i,j,temp;
    printf("enter the size of n");
    scanf("%d",&n);
    int arr[n];
    printf("enter the array elements");
    for(i=0;i<n;i++) {
        scanf("%d",&arr[i]);
    }
    printf("enter the size of m");
    scanf("%d",&m);
    int arr2[m];
    printf("enter the array elements");
    for(i=0;i<m;i++) {
        scanf("%d",&arr2[i]);
    }
    if(n != m) {
        printf("NO");
        return 0;
    }
    for(i = 0; i < n-1; i++) {
        for(j = i+1; j < n; j++) {
            if(arr[i] > arr[j]) {
                temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }
    for(i = 0; i < m-1; i++) {
        for(j = i+1; j < m; j++) {
            if(arr2[i] > arr2[j]) {
                temp = arr2[i];
                arr2[i] = arr2[j];
                arr2[j] = temp;
            }
        }
    }
    for(i = 0; i < n; i++) {
        if(arr[i] != arr2[i]) {
            printf("NO");
            return 0;
        }
    }
    printf("YES");

    return 0;
}
